{
  "name": "@xoram/utils",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "build:code:prod": "vite build --mode production",
    "build:code:dev": "NODE_ENV=development vite build --mode development",
    "build:code": "node ../../script/build-code.script.mjs",
    "build:types": "node ../../script/build-types.script.mjs -- --preserve-individual-files",
    "build:synthetic-index": "node ./script/build-synthetic-index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ragnar-Oock/xoram.git",
    "directory": "packages/utils"
  },
  "private": true,
  "files": [
    "./dist",
    "./types/*.d.ts",
    "tsdoc-metadata.json"
  ],
  "exports": {
    "./*": {
      "xoram:dev": {
        "types": "./src/*/index.ts",
        "default": "./src/*/index.ts"
      },
      "types": "./types/*/index.d.ts",
      "import": "./dist/*/index.ts"
    }
  }
}
